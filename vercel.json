{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/keep-alive",
      "dest": "/src/api/keep-alive.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.ts"
    }
  ],
  "crons": [
    {
      "path": "/api/keep-alive",
      "schedule": "*/5 * * * *"
    }
  ]
} 